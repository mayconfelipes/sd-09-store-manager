{
  "$async": true,
  "type": "object",
  "properties": {
    "itensSold": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "productId": {
            "type": "string",
            "minLength": 5,
            "productIdExists": true
          },
          "quantity": {
            "type": "number",
            "exclusiveMinimum": 0
          }
        },
        "additionalProperties": false,
        "required": ["productId", "quantity"]
      }
    }
  },
  "errorMessage": "Wrong product ID or invalid quantity"
}
